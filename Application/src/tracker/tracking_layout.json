{ "objects": [

    {
        "type":"button",
        "text":"Test",
        "pos":[300,100],
        "size":[100,40],
        "action":"change_scene:'starting-scene'"
    },

    {
        "type":"collection",
        "pos":[10,90],
        "size":[200,350],
        "fill":[50,50,50,125],
        "children": [
            { "type":"stext",
              "text":"<b>{global.individual_prefix}0</b>",
              "pos":[10,10]
            }
        ]
    },

    {
        "type":"vlayout",
        "pad":[0,10,0,0],
        "children":[
            {
                "type":"hlayout",
                "pad":[10,0,0,0],
                "children":[
                    {
                        "type":"button",
                        "action":"set:analysis_paused:{not:{global.analysis_paused}}",
                        "text":"{if:global.analysis_paused:'<sym>▶</sym>':'<sym>⏸</sym>'}",
                        "size":[25,25],
                        "font":{"size":0.8},
                        "fill":[10,0,0,0],
                        "line":[10,0,0,0],
                        "color":"{if:global.analysis_paused:[30,80,30,100]:[200,120,80,100]}"
                    },
                    {
                        "type": "stext",
                        "text": "<b>{global.source}</b> — <a>{global.gui_frame}</a>/<a>{-:{global.video_length}:1}</a> {if:global.analysis_paused:'(<orange>paused</orange>)':'@ <a>{fps}</a>fps'}",
                        "font":{ "size":0.65 },
                        "shadow":0.5,
                        "pad":[-5,0,0,7.5]
                    }
                ]
            },
            {
                "type":"collection",
                "children":[
                    {
                        "type": "rect",
                        "pos": [10, -10],
                        "size": "[{+:{window_size.x}:-20},30]",
                        "fill": [50,50,50,125],
                        "line": [50,50,50,255],
                        "clickable":true,
                        "action": "set:gui_frame:{min:{-:{global.video_length}:1}:{*:{/:{-:{mouse.x}:10}:{-:{window_size.x}:25}}:{-:{global.video_length}:0.5}}}"
                    },


                    {
                        "type":"each",
                        "var":"consec",
                        "do": {
                            "type": "rect",
                            "pos": "{addVector:[{round:{*:{/:{i.from}:{{-:{global.video_length}:1}}}:{+:{window_size.x}:-25}}},0]:[12.5,-10]}",
                            "size": "{addVector:[{round:{*:{/:{-:{i.to}:{i.from}}:{{-:{global.video_length}:1}}}:{-:{window_size.x}:25}}},30]:[0,0]}",
                            "fill": "{clrAlpha:{i.color}:50}",
                            "line":"{clrAlpha:{i.color}:100}"
                        }
                    },

                    {
                        "type": "rect",
                        "pos": "{addVector:[{round:{*:{/:{global.gui_frame}:{{-:{global.video_length}:1}}}:{+:{window_size.x}:-25}}},-12]:[12.5,0]}",
                        "origin": [0.5,0],
                        "size": [5,34],
                        "fill": [150,150,150,225],
                        "line":[50,50,50,255]
                    }
                ]
            },
            {
                "type":"each",
                "var":"consec",
                "do": {
                    "type": "text",
                    "text": "{i.from} - {i.to} = {-:{i.to}:{i.from}}"
                }
            }
            
        ]
    },

    {
        "type":"settings",
        "var":"gui_frame",
        "size":[100,40],
        "pos":[300,150],
        "fill":[50,50,50]
    },
    {
        "type":"settings",
        "var":"video_length",
        "size":[100,40],
        "pos":[300,200],
        "fill":[50,50,50]
    },

    {
        "type":"hlayout",
        "pos":"{addVector:[0,{-:{window_size.y}:5}]:0}",
        "origin":[0,1],
        "pad":[10,10,0,0],
        "children":[
            {
                "type":"list",
                "pos":[0,0],
                "size":[200,140],
                "fill":[50,50,50],
                "foldable":true,
                "folded":true,
                "text":"test",
                "item_height":1,
                "font":{"size":0.6},
                "items":[
                    {
                        "text": "hi",
                        "detail": "=>0",
                        "action": "set:'gui_frame':0"
                    },
                    {
                        "text": "to the mid",
                        "detail": "=>50",
                        "action": "set:gui_frame:{min:{+:{global.gui_frame}:1}:{global.video_length}}"
                    },
                    {
                        "text":"selections",
                        "detail":"{if:global.gui_show_selections:'hide':'show'}",
                        "action":"set:gui_show_selections:{not:{global.gui_show_selections}}"
                    }
                ]

            },
            {
                "type":"settings",
                "var":"",
                "font":{"size":0.75,"align":"center"},
                "fill":[50,50,50],
                "line":[75,75,75],
                "size":[400,40],
                "color":[225,225,225],
                "desc":"",
                "highlight_clr":[75,75,75,255]
            }
        ]
    }
]}