{ "objects": [
    {
        "type":"button",
        "text":"Test",
        "pos":[300,100],
        "size":[100,40],
        "action":"change_scene:'starting-scene'"
    },
    {
        "type":"button",
        "text":"{window_size} {global.app_name}",
        "pos":"{addVector:{global.gui_zoom_limit}:[10,20]}",
        "size":"{addSize:{global.gui_zoom_limit}:0}"
    },

    {
        "type":"collection",
        "pos":[10,70],
        "size":[200,350],
        "fill":[50,50,50,125],
        "children": [
            { "type":"stext",
              "text":"<b>{global.individual_prefix}0</b>",
              "pos":[10,10]
            }
        ]
    },

    {
        "type": "rect",
        "pos": [10, 10],
        "size": "[{+:{window_size.x}:-20},40]",
        "fill": [50,50,50,125],
        "line": [150,150,150,255],
        "clickable":true,
        "action": "set-frame:{*:{/:{-:{mouse.x}:10}:{-:{window_size.x}:25}}:{global.video_length}}"
    },
    {
        "type": "rect",
        "pos": "{addVector:[{round:{*:{/:{global.gui_frame}:{global.video_length}}:{+:{window_size.x}:-25}}},10]:[12.5,0]}",
        "origin": [0.5,0],
        "size": [5,40],
        "fill": [150,150,150,125],
        "line":[200,200,200,200]
    },

    {
        "type":"settings",
        "var":"gui_frame",
        "size":[100,40],
        "pos":[300,150],
        "fill":[50,50,50]
    },
    {
        "type":"settings",
        "var":"video_length",
        "size":[100,40],
        "pos":[300,200],
        "fill":[50,50,50]
    },
    {
        "type":"settings",
        "var":"",
        "origin":[0,1],
        "pos":"{addVector:[10,{-:{window_size.y}:10}]:0}",
        "font":{"size":0.75,"align":"center"},
        "fill":[50,50,50],
        "line":[75,75,75],
        "size":[400,40],
        "color":[225,225,225],
        "desc":"",
        "highlight_clr":[75,75,75,255]
    },

    {
        "type":"each",
        "pos":[0,0],
        "size":[100,100],
        "var":"fishes",
        "do": {
            "type":"circle",
            "pos":"{i.pos}",
            "fill":[125,125,125]
        }
    },

    {
        "type":"each",
        "pos":[0,0],
        "size":[100,100],
        "var":"fishes",
        "do": {
            "type":"text",
            "pos":"{minVector:{addVector:{addVector:{mulVector:{i.i}:[0,20]}:{mulVector:{i.pos}:0.1}}:[50,50]}:{addVector:{window_size}:[-150,-50]}}",
            "text":"{i.name} => {i.detail}",
            "font":{ "size":0.75 },
            "color":[125,125,125]
        }
    }
]}