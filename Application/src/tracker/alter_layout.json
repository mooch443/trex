{"objects":[
	{ 
		"type":"vlayout",
		"pos":[20,0],
		"scale":[1.0,1.0],
		"children": [
			{ 
				"type":"hlayout", 
				"children": [
					{ "type":"button", "text":"Quit", "action":"QUIT" },
					{ "type":"button", "text":"Filter", "action":"FILTER" },
					{ "type":"button", "text":"Reset", "action":"RESET" },
					{ "type":"text", "text":"total:{fps:2d}fps net:{net_fps}fps video:{vid_fps}", "font":{"size":0.75,"style":"regular"}, "color":[255,255,255] }
				]
			},

			{ "type":"text", "text":"  resolution: {video:resolution} | processed resolution: {global:image_width} | actual: {actual_frame} | written: {video:frame}", "font":{"size":0.5}, "color":[100,230,255] }
		]
	},

	{
		"type":"each",
		"var":"fishes",
		"do": {
			"type": "collection",
			"pos":"{i:pos}",
			"origin":[0,0],
			"children": [
				{
					"type":"condition",
					"var":"{i:tracked}",
					"then": {"type":"stext","text":"<b><nr>{i:id}</nr></b>: {i:type} (<i>{i:p}</i>)","font":{"size":0.5},"origin":[0,1]},
					"else": {"type":"stext","text":"<ref><b>{i:id}</b>: {i:type} (<i>{i:p}</i>)</ref>","font":{"size":0.5},"origin":[0,1]}
				},
				{
					"type":"condition",
					"var":"{i:tracked}",
					"then": {"type":"rect","pos":[0,0],"size":"{i:size}","fill":[0,255,0,30],"line":"{i:color}","origin":[0,0]},
					"else": {"type":"rect","pos":[0,0],"size":"{i:size}","fill":[50,50,50,30],"line":[200,200,200,150],"origin":[0,0]}
				}
			]
		}
	}
]}